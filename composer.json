{
    "name": "vagrant/blackjack_cli",
    "description": "blackjack game",
    "type": "project",
    "license": "MIT",
    "require": {},
    "require-dev": {
      "squizlabs/php_codesniffer": "3.2.3",
        "phpdocumentor/phpdocumentor": "^2.9",
        "symfony/console": "*",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "src\\": "src/",
            "vendor\\": "vedor/",
            "PHPUnit\\": "vendor/phpunit/phpunit/src"
        }
    },
    "scripts": {
      "start":[
        "php ./src/main.php"
      ],
      "test": [
        "./vendor/bin/phpunit --bootstrap ./vendor/autoload.php --testdox test/"
      ],
      "reload": [
        "composer dumpautoload"
      ]
    }
}
